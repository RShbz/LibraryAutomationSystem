package library;

import static org.junit.jupiter.api.Assertions.assertEquals;

import org.junit.jupiter.api.Test;

import com.sahabt.library.domain.borrow.Borrow;
import com.sahabt.library.domain.catalog.Catalog;
import com.sahabt.library.domain.credicard.CrediCard;
import com.sahabt.library.domain.credicard.ExpirationDate;

public class BorrowTest {

	@Test
	void test() {
		
	var booking1 = new Booking.Builder()
			.identityNo("11111")
			.bookId("19910")
			.isbn("12AA1")
			.build();
	}
	
	assertEquals("11111", booking1.getIdentityNo().getIdentityNo());
	assertEquals("19910", booking1.getBookId().BookId());
	assertEquals("12AA1", booking1.getIsbn().getIsbn());

}
